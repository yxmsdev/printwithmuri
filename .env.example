# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Paystack (Required for payment processing)
PAYSTACK_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_PAYSTACK_PUBLIC_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx

# App
NEXT_PUBLIC_URL=http://localhost:3000

# Email (Resend)
RESEND_API_KEY=

# Security - REQUIRED in production
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
RESET_TOKEN_SECRET=

# Server-Side Slicer (CuraEngine)
# Path to CuraEngine binary (default: CuraEngine in PATH)
CURAENGINE_PATH=/usr/local/bin/CuraEngine
# Slicing timeout in milliseconds (default: 60000 = 60 seconds)
SLICER_TIMEOUT=60000
# Temporary directory for slicing files (default: /tmp/slicing)
SLICER_TEMP_DIR=/tmp/slicing
